    <div class="sticky" id="nav2">
      <div id="nav2left">
        <h3>JORDAN SHOES & SNEAKERS</h3>
      </div>
      <div id="nav2right">
        <a href="">
          <h4>Hide Filter</h4>
          <svg aria-hidden="true" class="icon-filter-ds" focusable="false" viewBox="0 0 24 24" role="img" width="24px" height="24px" fill="none"><path stroke="currentColor" stroke-width="1.5" d="M21 8.25H10m-5.25 04"></path><path stroke="currentColor" stroke-width="1.5" d="M7.5 6v0a2.25 2.25 0 100 4.5 2.25 2.25 0 000-4.5z" clip-rule="evenodd"></path><path stroke="currentColor" stroke-width="1.5" d="M3 15.75h10.75m5 0H21"></path><path stroke="currentColor" stroke-width="1.5" d="M16.5 13.5v0a2.25 2.25 0 100 4.5 2.25 2.25 0 000-4.5z" clip-rule="evenodd"></path></svg>
        </a>
        <a href="">
          <h4>Sort By</h4>
          <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="24" height="24"><path fill="none" d="M0 0h24v24H0z"/><path d="M12 13.172l4.95-4.95 1.414 1.414L12 16 5.636 9.636 7.05 8.222z"/></svg>
        </a>
      </div>
    </div>
    <div id="mainsection">
      <div id="mainfiltersection">
        <div id="shoetop">
          <a href="/filteredProduct/low">Low Top</a>
          <a href="/filteredProduct/mid">Mid Top</a>
          <a href="/filteredProduct/high">High Top</a>
        </div>
        <hr>
        <div id="gender">
          <div id="Ctop">
            <h4>Gender</h4>
            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="24" height="24"><path fill="none" d="M0 0h24v24H0z"/><path d="M12 11.828l-2.828 2.829-1.415-1.414L12 9l4.243 4.243-1.415 1.414L12 11.828z"/></svg>
          </div>
          <div id="Cbot">
            <a href="/filteredProduct/Men">Men</a>
            <a href="/filteredProduct/Women">Women</a>
            <a href="/filteredProduct/Unisex">Unisex</a>
          </div>
        </div>
        <hr>
        <div id="kids">
          <div id="Ctop">
            <h4>Kids</h4>
            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="24" height="24"><path fill="none" d="M0 0h24v24H0z"/><path d="M12 11.828l-2.828 2.829-1.415-1.414L12 9l4.243 4.243-1.415 1.414L12 11.828z"/></svg>
          </div>
          <div id="Cbot">
            <a href="/filteredProduct/Boys">Boys</a>
            <a href="/filteredProduct/Girls">Girls</a>
          </div>
        </div>
        <hr>
        <div id="shopbyprice">
          <div id="Ctop">
            <h4>Shop By Price</h4>
            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="24" height="24"><path fill="none" d="M0 0h24v24H0z"/><path d="M12 11.828l-2.828 2.829-1.415-1.414L12 9l4.243 4.243-1.415 1.414L12 11.828z"/></svg>
          </div>
          <div id="Cbot">
            <a href="/filteredProduct/under100">Under $100</a>
            <a href="/filteredProduct/100to150">$100-$150</a>
            <a href="/filteredProduct/150to200">$150-$200</a>
            <a href="/filteredProduct/over200">over $200</a>
            
          </div>
        </div>
        <div id="color">
          <div id="Ctop">
            <h4>Color</h4>
            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="24" height="24"><path fill="none" d="M0 0h24v24H0z"/><path d="M12 11.828l-2.828 2.829-1.415-1.414L12 9l4.243 4.243-1.415 1.414L12 11.828z"/></svg>
          </div>
          <div id="Cbot">
            <a  href="/filteredProduct/Purple" id="colors">
              <div class="circle" id="Purple"></div>
              <div id="cname">Purple</div>
            </a>
            <a href="/filteredProduct/Black" id="colors">
              <div class="circle" id="Black"></div>
              <div id="cname">Black</div>
            </a>
            <a href="/filteredProduct/Red" id="colors">
              <div class="circle" id="Red"></div>
              <div id="cname">Red</div>
            </a>
            <a href="/filteredProduct/Blue" id="colors">
              <div class="circle" id="Blue"></div>
              <div id="cname">Blue</div>
            </a>
            <a href="/filteredProduct/White" id="colors">
              <div class="circle" id="White"></div>
              <div id="cname">White</div>
            </a>
            <a href="/filteredProduct/Brown" id="colors">
              <div class="circle" id="Brown"></div>
              <div id="cname">Brown</div>
            </a>
            <a href="/filteredProduct/Green" id="colors">
              <div class="circle" id="Green"></div>
              <div id="cname">Green</div>
            </a>
            <a href="/filteredProduct/Grey" id="colors">
              <div class="circle" id="Grey"></div>
              <div id="cname">Grey</div>
            </a>
            <a href="/filteredProduct/Pink" id="colors">
              <div class="circle" id="Pink"></div>
              <div id="cname">Pink</div>
            </a>
          </div>
        </div>
        <div id="extra"></div>
      </div>
      <div class="container" id="mainproductsection">
        <% product.forEach(function(elem){ %>
        <a href="/product/<%=elem._id%>" class="card" style="width: 24rem;">
          <img class="card-img-top" src=<%= elem.productImage %> alt="Card image cap">
          <div class="card-body">
            <h5 class="card-title"><%= elem.productName %></h5>
            <h6 class="card-text">
              <%if(elem.productCat==="Men"){%>
                Men 's Shoes
                <%} %>
                <%if(elem.productCat==="Women"){%>
                  Women 's Shoes
                  <%} %>
                  <%if(elem.productCat==="Boys"){%>
                    Boys 's Shoes
                    <%} %>
                    <%if(elem.productCat==="Girls"){%>
                      Girls 's Shoes
                      <%} %>
                  </h6>
            <h6 class="card-text"> <%= elem.color.length %> color</h6>
            <h6 class="card-text">$<%= elem.price %></h6>
          </div>
        </a>
        <%})%>
    </div>
    <div id="footer"></div>
  </div>

</body>
</html>